<extension xmlns="http://ns.adobe.com/air/extension/3.1">
    <id>com.culpritgames.loadingSpinner</id>
    <versionNumber>1.0.0</versionNumber>
    <platforms>
        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libLoadingDialogExtension.a</nativeLibrary>
                <initializer>LoadingSpinnerExtensionIntitializer</initializer>
                <finalizer>LoadingSpinnerExtensionFinalizer</finalizer>
            </applicationDeployment>
        </platform>
		<platform name="iPhone-x86">
            <applicationDeployment>
                <nativeLibrary>libLoadingDialogExtension-sim.a</nativeLibrary>
                <initializer>LoadingSpinnerExtensionIntitializer</initializer>
                <finalizer>LoadingSpinnerExtensionFinalizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="default">
            <applicationDeployment/>
        </platform>     
	</platforms>
</extension>